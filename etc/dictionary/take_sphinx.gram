#JSGF V1.0;
grammar take_sphinx;
public <rule> = <command> <place> | <command2> | <noise>;
<command> = take this bag to the;
<place> = kitchen | bed_room | living_room;
<command2> = please say again;
<noise> = a | b | c | d | e | f | g | h | i | j | k | l | m | n | p | q | r | s | t | u | v | w | s | y | z | ka | sa | ta | ha | ma | ya | ra | wa;
