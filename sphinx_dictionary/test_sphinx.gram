#JSGF V1.0;
grammar test_sphinx;
public <rule> = <command>| <where> | <here> <place> | <go> <place> | <noise>;
<command> = follow me | stop following me | yes | no;
<where> = Where are you;
<here> = Here is the;
<go> = Please go to the;
<place> = kitchen | bed_room | living_room | entrance | car;
<noise> = a | b | c | d | e | f | g | h | i | j | k | l | m | n | p | q | r | s | t | u | v | w | s | y | z | ka | sa | ta | ha | ma | ya | ra | wa;
